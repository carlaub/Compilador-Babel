funcio printaVector(perval vec1: vector[0..N-1] de sencer, perref vec2: vector[0..N-1] de sencer): logic;
func
	i = 0;
	cicle
		escriure(v0[i],"-");
		i = i + 1;
	fins i == N;
	escriure("\n");
	i = 0;
	cicle
		escriure(vec1[i],"-");
		i = i + 1;
	fins i==N;
	escriure("\n");
	i = 0;
	cicle
		escriure(vec2[i]*2,"-");
		vec2[i] = vec2[i] * 2;
		i = i + 1;
	fins i==N;
	escriure("\n");
	retornar cert;
fifunc;

